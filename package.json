    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "cross-env": "^5.1.4",
    "gh-pages": "^2.0.1",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-scripts-ts": "^3.1.0",
    "rollup": "^1.10.1",
    "rollup-plugin-babel": "^4.3.2",
